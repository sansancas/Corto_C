#include <stdio.h>
int main() {
int i, n;
  int a = 0, b = 1;
  int sig = a + b;
  printf("ingrese la cantidad de veces que desea realizar la secuencia fibonaci: ");
  scanf("%d", &n);
  printf("Serie Fibonaci: %d, %d, ", a, b);
  for (i = 3; i <= n; ++i) {
    printf("%d, ", sig);
    a = b;
    b = sig;
    sig = a + b;
  }
  return 0;
}
